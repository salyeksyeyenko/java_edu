/* 
Гадание на долларовый счет
*/

import java.io.*;

public class Solution {
    public static void main(String[] args) throws Exception {
        String s = "S";
        int a = 10;
        int b = 9;

        while (a > 0){
            while (b > 0){
                System.out.print(s);
                b--;
            }
            System.out.println(s);
            b = 9;
            a--;
        }
    }
}
